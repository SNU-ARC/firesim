usage: firesim [-h] [--helpfull]
               {managerinit,infrasetup,boot,kill,runworkload,buildafi,tar2afi,runcheck,launchrunfarm,terminaterunfarm,shareagfi}

FireSim Simulation Manager.

positional arguments:
  {managerinit,infrasetup,boot,kill,runworkload,buildafi,tar2afi,runcheck,launchrunfarm,terminaterunfarm,shareagfi}
                        Management task to run.

optional arguments:
  -h, --help            show this help message and exit
  --helpfull            show full help message and exit

buildtools.buildconfigfile:
  -b,--buildconfigfile: Optional custom build config file.
    (default: 'config_build.yaml')
  -s,--buildfarmconfigfile: Optional custom build farm config file.
    (default: 'config_build_farm.yaml')
  -r,--buildrecipesconfigfile: Optional custom build recipe config file.
    (default: 'config_build_recipes.yaml')
  -t,--launchtime: Give the "Y-m-d--H-M-S" prefix of results-build directory.
    Useful for tar2afi when finishing a partial buildafi

runtools.run_farm:
  -q,--[no]forceterminate: For terminaterunfarm, force termination without
    prompting user for confirmation.
    (default: 'false')
  -f,--terminatesomef116: Only used by terminatesome. Terminates this many of
    the previously launched f1.16xlarges.
    (an integer)
  -g,--terminatesomef12: Only used by terminatesome. Terminates this many of the
    previously launched f1.2xlarges.
    (an integer)
  -i,--terminatesomef14: Only used by terminatesome. Terminates this many of the
    previously launched f1.4xlarges.
    (an integer)
  -m,--terminatesomem416: Only used by terminatesome. Terminates this many of
    the previously launched m4.16xlarges.
    (an integer)

runtools.runtime_config:
  -a,--hwdbconfigfile: Optional custom HW database config file.
    (default: 'config_hwdb.yaml')
  -x,--overrideconfigdata: Override a single value from one of the the RUNTIME
    e.g.: --overrideconfigdata "target-config link-latency 6405".
    (a whitespace separated list)
  -c,--runtimeconfigfile: Optional custom runtime/workload config file.
    (default: 'config_runtime.yaml')

absl.flags:
  --flagfile: Insert flag definitions from the given file into the command line.
    (default: '')
  --undefok: comma-separated list of flag names that it is okay to specify on
    the command line even if the program does not define a flag with that name.
    IMPORTANT: flags in this list that have arguments MUST use the --flag=value
    format.
    (default: '')
